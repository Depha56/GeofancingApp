{
  "expo": {
    "name": "GeofancingApp",
    "slug": "GeofancingApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store",
      "@rnmapbox/maps",
        {
          "RNMapboxMapsDownloadToken": "sk.eyJ1Ijoic2F2ZXVyMSIsImEiOiJjbWMxb3Qxa2QwN3kwMm5xdzVyeXkxd2d4In0.zMnNAn8GpaVygxSDDKZ3OQ",
          "RNMapboxMapsVersion": "10.1.39"
        }
    ],
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#103060"
    },
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.bsaveur.GeofancingApp"
    }
  }
}
