{
  "expo": {
    "name": "GeofancingApp",
    "slug": "GeofancingApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsDownloadToken": "sk.eyJ1Ijoic2F2ZXVyMSIsImEiOiJjbWMxeGk4bjUwMjUxMmtxdzVhZ3B1dWp5In0.KHsxDphXjmMO9fe5KKSJgg",
          "locationWhenInUsePermission": "Show current location on map."
        }
      ]
    ],
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#103060"
    },
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.bsaveur.GeofancingApp"
    },
    "ios": {
      "bundleIdentifier": "com.bsaveur.GeofancingApp"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "03c8eec1-f7fe-4ff0-b909-cfc737f46d74"
      }
    }
  }
}
